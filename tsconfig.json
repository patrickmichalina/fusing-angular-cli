{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "lib": ["es6", "dom"],
    "typeRoots": ["tools/manual-typings", "node_modules/@types"]
  }
}
