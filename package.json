  "ts:main": "index.ts",
  "typings": "index.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fusing-angular/fusing-angular"
  },
  "author": {
    "name": "Patrick Michalina",
    "email": "patrickmichalina@mac.com",
